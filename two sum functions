def my_function(n,n1):
    t=int(input("enter the target:"))
    for i in range(0,n):
        for j in range(i+1,n):
            if n1[i]+n1[j]==t:
                print([i,j])
                break
n=int(input("enter the size of array:"))
n1=[]
for k in range(0,n):
    s=int(input())
    n1.append(s)
my_function(n,n1)

output:
enter the size of array:4
2
7
11
13
enter the target:9
[0, 1]
